# mount point       fstype    device                                                                flags
/system             ext4      /dev/block/bootdevice/by-name/system					flags=slotselect;backup=1;display="System";wipeingui;
/system_image       emmc      /dev/block/bootdevice/by-name/system					flags=slotselect;
/vendor             ext4      /dev/block/bootdevice/by-name/vendor					flags=slotselect;display="Vendor";backup=1;wipeingui;
/vendor_image       emmc      /dev/block/bootdevice/by-name/vendor					flags=slotselect;
/data               ext4      /dev/block/bootdevice/by-name/userdata				flags=encryptable=footer;backup=1;display="Data";
/sdcard1            auto      /dev/block/mmcblk1p1									flags=display="MicroSD";storage;wipeingui;removable;
/boot               emmc      /dev/block/bootdevice/by-name/boot					flags=display="Boot";slotselect;backup=1;flashimg=1;
/misc               emmc      /dev/block/bootdevice/by-name/misc
/xbl                emmc      /dev/block/bootdevice/by-name/xbl						flags=slotselect;
/xbl_config         emmc      /dev/block/bootdevice/by-name/xbl_config				flags=slotselect;
/tz                 emmc      /dev/block/bootdevice/by-name/tz						flags=slotselect;
/aop                emmc      /dev/block/bootdevice/by-name/aop						flags=slotselect;
/hyp                emmc      /dev/block/bootdevice/by-name/hyp						flags=slotselect;
/keymaster          emmc      /dev/block/bootdevice/by-name/keymaster				flags=slotselect;
/cmnlib             emmc      /dev/block/bootdevice/by-name/cmnlib					flags=slotselect;
/cmnlib64           emmc      /dev/block/bootdevice/by-name/cmnlib64				flags=slotselect;
/firmware           emmc      /dev/block/bootdevice/by-name/modem					flags=slotselect;
/bt_firmware        vfat      /dev/block/bootdevice/by-name/bluetooth				flags=display="Bluetooth";slotselect;
/dsp                emmc      /dev/block/bootdevice/by-name/dsp						flags=slotselect;
/abl                emmc      /dev/block/bootdevice/by-name/abl						flags=slotselect;
/dtbo               emmc      /dev/block/bootdevice/by-name/dtbo					flags=display="Dtbo";slotselect;backup=1;flashimg=1;
/persist            ext4      /dev/block/bootdevice/by-name/persist
/persist_image      emmc      /dev/block/bootdevice/by-name/persist					flags=display="Persist image";backup=1;flashimg=1;
/devcfg             emmc      /dev/block/bootdevice/by-name/devcfg					flags=slotselect;
/qupfw              emmc      /dev/block/bootdevice/by-name/qupfw					flags=slotselect;
/storsec            emmc      /dev/block/bootdevice/by-name/storsec					flags=slotselect;
/vbmeta             emmc      /dev/block/bootdevice/by-name/vbmeta					flags=slotselect;
/splash             emmc      /dev/block/bootdevice/by-name/splash					flags=display="Splash";slotselect;backup=1;flashimg=1;
/zplash             emmc      /dev/block/bootdevice/by-name/zplash					flags=display="Zplash";slotselect;backup=1;flashimg=1;
/nvdef              emmc      /dev/block/bootdevice/by-name/nvdef					flags=slotselect;
/sutinfo            emmc      /dev/block/bootdevice/by-name/sutinfo
/logfs              vfat      /dev/block/bootdevice/by-name/logfs					flags=display="Logfs";
/systeminfo         emmc      /dev/block/bootdevice/by-name/systeminfo				flags=slotselect;

# USB Storage
/usb_otg       	    vfat      /dev/block/sda1                    /dev/block/sda		flags=display="USB-Storage";storage;removable;wipeingui
